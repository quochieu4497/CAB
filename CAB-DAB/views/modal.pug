#addCompo.modal(tabindex='-1', role='dialog', aria-labelledby='exampleModalCenterTitle', aria-hidden='true')
	.modal-dialog.modal-md.modal-dialog-centered(role='document')
		.modal-content
				.modal-header
					h5#exampleModalLabel.modal-title Thêm Components
					button.close(type='button', data-dismiss='modal', aria-label='Close')
						span(aria-hidden='true') ×
				form#formAddComponent(enctype='multipart/form-data', method='POST')
					.modal-body.clearfix
						.row
							.col-6
								.form-group.mb-2
									label Tên components
									input#nameCompo.form-control(type='text', name='nameCompo', placeholder="Tên components VD: 'Header 3'", required='')
							.col-6
								.form-group.mb-2
									label Độ cao mặc định
									.input-group.mb-3
										input#heightCompo.form-control(type='text', name='heightCompo', placeholder='Không bắt buộc')
										.input-group-append
											span.input-group-text px
							.col-12
								.form-group.mb-2
									label Chú thích
									input#nameNoted.form-control(type='text', name='nameNoted', placeholder="", required='')
							.col-6
								.form-group.mb-2
									label Chọn menu
									select.form-control(name='selectCompo', required='')
										option(selected='', disabled='') Vui lòng chọn
							.col-6
								.form-group.mb-2
									label Upload
									.custom-file
										input#imgUploader.custom-file-input(type='file', name='imgUploader', required='')
										label.custom-file-label.justify-content-start(for='imgUploader') Chọn ảnh
						.row
							.col
								.bg-light.p-2.textresult ...
					.modal-footer
						input#comkey(type='hidden', name='comkey')
						input#commain(type='hidden', name='commain')
						input#comnum(type='hidden', name='comnum')
						button.btn.btn-success(type='button') Thêm Component
